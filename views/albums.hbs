<button onclick="window.window.history.back();">Go back to artists</button>
<div class="header">
    <h2>{{artists_name}}</h2>
</div>
<div class="card">
    {{#results}}
    <div class="results">
        <img src="{{images.url}}" alt="artist">
        <p class="artist-name">{{name}}</p>
        <a class="button" href="/tracks/{{id}}">View Tracks</a>
    </div>
    {{/results}}
</div>
